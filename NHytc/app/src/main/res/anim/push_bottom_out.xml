<?xml version="1.0" encoding="utf-8"?>
<!-- 上下滑出式 -->
<set xmlns:android="http://schemas.android.com/apk/res/android" >


    <rotate
        android:fromDegrees="0"
        android:toDegrees="135"
        android:duration="230"
        android:pivotX="50%"
        android:pivotY="50%"
        android:fillAfter="true"
        />
</set>